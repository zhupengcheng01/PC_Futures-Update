<UserControl x:Class="PC_Futures.ANXINYI.UCBankTransfer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PC_Futures.ANXINYI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <StackPanel Orientation="Horizontal">
        <Button Name="InMoney" HorizontalAlignment="Left"  Padding="15,1" Margin="50,0,0,3" VerticalAlignment="Center"  Width="100" Height="25">入金</Button>
        <Button Name="OutMoney" HorizontalAlignment="Left"  Padding="15,1" Margin="30,0,0,3" VerticalAlignment="Center"  Width="100" Height="25">出金</Button>
        </StackPanel>
    </Grid>
</UserControl>
